from zlib import _Compress, _Decompress

from _typeshed import ReadableBuffer

class ZlibCompressor:
    z: _Compress
    def __init__(self) -> None: ...
    def __call__(self, data: ReadableBuffer) -> bytes: ...

class ZlibDecompressor:
    z: _Decompress
    def __init__(self) -> None: ...
    def __call__(self, data: ReadableBuffer) -> bytes: ...
